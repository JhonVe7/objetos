package clase;

import cuenta.CCuentaAhorro;

public class Test {
	
	public static void main(String[] args) {
		CCuenta cuenta01 = new CCuenta();
		CCuenta cuenta02 = new CCuenta("Pedro","00000001",6000,3.5);
		cuenta01.asignarNombre("Maria");
		cuenta01.asignarCuenta("00009");
		cuenta01.asignarTipoDeIntereses(2.5);
		
		cuenta01.ingreso(2000);
		cuenta01.reintegro(900);
		
		System.out.println(cuenta01.obtenerNombre());
		System.out.println(cuenta01.obtenerCuenta());
		System.out.println(cuenta01.estado());
		

		
		System.out.println(cuenta02.obtenerNombre());
		System.out.println(cuenta02.obtenerCuenta());
		System.out.println(cuenta02.estado());
		
		//herencia
		CCuentaAhorro cuenta01 = new CCuentaAhorro("alonso",);
		//cobrar cuota mante
		cuanta01.reintegro(cuenta01.obtenerCuotaManten());
		cuenta01.ingreso(6000);
		cuenta01.reintegro(2000);
	}

}
